{
  "manifest_version": 3,
  "name": "Task Manager",
  "version": "1.0",
  "description": "A brief description of your extension",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js","dashboard.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "matches": ["<all_urls>"]
    }
}
